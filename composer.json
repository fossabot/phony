{
    "name": "deligoez/phony",
    "description": "A PHP Package that generates phony data",
    "keywords": [
        "package",
        "faker",
        "phony"
    ],
    "homepage": "https://github.com/deligoez/phony",
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/deligoez/phony/issues",
        "source": "https://github.com/deligoez/phony"
    },
    "authors": [
        {
            "name": "Yunus Emre Delig√∂z",
            "email": "ye@deligoz.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Deligoez\\Phony\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deligoez\\Phony\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration phpunit.xml.dist --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
