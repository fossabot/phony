{
    "name": "phonyland/phony",
    "description": "Generate massive amount of realistic fake data in multiple locales",
    "keywords": ["package", "faker", "phony"],
    "homepage": "https://github.com/phonyland/phony",
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/phonyland/phony/issues",
        "source": "https://github.com/phonyland/phony"
    },
    "authors": [
        {
            "name": "Yunus Emre Delig√∂z",
            "email": "ye@deligoz.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "pestphp/pest": "^0.1.2",
        "phpunit/phpunit": "^9.0",
        "simpleregex/srl-php": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Phonyland\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phonyland\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest --colors=always",
        "coverage": "vendor/bin/pest --configuration phpunit.xml --colors=always --coverage --coverage-clover 'reports/clover.xml'"
    },
    "config": {
        "sort-packages": true
    }
}
