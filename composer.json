{
    "name": "phonyphp/phony",
    "description": "Phony is a PHP package that can generate a wide range of fake data",
    "keywords": ["package", "faker", "phony"],
    "homepage": "https://github.com/phonyphp/phony",
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/phonyphp/phony/issues",
        "source": "https://github.com/phonyphp/phony"
    },
    "authors": [
        {
            "name": "Yunus Emre Delig√∂z",
            "email": "ye@deligoz.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Phony\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phony\\Test\\": "test"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration phpunit.xml.dist --colors=always"
    },
    "config": {
        "sort-packages": true
    }
}
