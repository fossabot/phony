{
    "name": "deligoez/phony",
    "description": "A Laravel Package that generates phony data",
    "keywords": [
        "laravel",
        "package",
        "faker",
        "dummy",
        "phony"
    ],
    "homepage": "https://github.com/deligoez/phony",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yunus Emre Delig√∂z",
            "email": "ye@deligoz.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Deligoez\\Phony\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deligoez\\Phony\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deligoez\\Phony\\PhonyServiceProvider"
            ],
            "aliases": {
                "Phony": "Deligoez\\Phony\\PhonyFacade"
            }
        }
    }
}
