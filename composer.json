{
    "name": "deligoez/phony",
    "description": "A Laravel Package that generates phony data",
    "keywords": [
        "laravel",
        "package",
        "faker",
        "phony"
    ],
    "homepage": "https://github.com/deligoez/phony",
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/deligoez/phony/issues",
        "source": "https://github.com/deligoez/phony"
    },
    "authors": [
        {
            "name": "Yunus Emre Delig√∂z",
            "email": "ye@deligoz.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0|^7.0"
    },
    "require-dev": {
        "brianium/paratest": "^4.0",
        "nunomaduro/collision": "^4.1",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Deligoez\\Phony\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deligoez\\Phony\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --configuration phpunit.xml.dist --colors=always",
        "para": "vendor/bin/paratest --configuration phpunit.xml.dist --colors"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deligoez\\Phony\\PhonyServiceProvider"
            ],
            "aliases": {
                "Phony": "Deligoez\\Phony\\PhonyFacade"
            }
        }
    }
}
